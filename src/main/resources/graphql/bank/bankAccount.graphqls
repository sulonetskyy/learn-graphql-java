type BankAccount {
    id: ID!
    client: Client
    currency: Currency!
    assets: [Asset]
    createdOn: Date
    createdAt: DateTime
}

type BankAccountEdge {
    cursor: String
    node: BankAccount
}

type PageInfo {
    startCursor: String!
    endCursor: String!
    hasPreviousPage: Boolean!
    hasNextPage: Boolean!
}

type BankAccountConnection {
    edges: [BankAccountEdge]
    pageInfo: PageInfo!
}
